<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.hibernateStudent.Student" table = "Student">
             
      <id name = "enrollmentNo" type = "int" column = "enrollmentNo">
         <generator class="native"/>
      </id>
      
      <property name = "name" column = "student_name" type = "string"/>
      <property name = "department" column = "department" type = "string"/>
     
      
   </class>
</hibernate-mapping>